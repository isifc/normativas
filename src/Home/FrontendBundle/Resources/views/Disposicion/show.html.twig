{% extends 'MWSimpleCrudGeneratorBundle::frontend.html.twig' %}

{% block title %}
{{ parent() }} - {{ 'views.show.show'|trans({'%entity%': 'Disposicion'}, 'MWSimpleCrudGeneratorBundle') }}
{% endblock %}

{% block page %}
    
 <div class="panel panel-default">
    <div class="panel-body">
        <h3>Disposición</h3>
           
                <div class="row">
                    <div class="col-md-3">
                        <label class="control-label">Oficina</label>
                    </div>
                    <div class="col-md-5">
                        <b>{{entity.oficina}}</b>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-3">
                        <label class="control-label">Temática</label>
                    </div>
                    <div class="col-md-5">
                        {{entity.tema}}
                    </div>
                </div>
                 <div class="row">
                    <div class="col-md-3">
                        <label class="control-label">Numero</label>
                    </div>
                    <div class="col-md-5">
                        {{entity.numero}}
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-3">
                        <label class="control-label">Fecha</label>
                    </div>
                    <div class="col-md-5">
                            {{ entity.fecha |date('d-m-Y') }}
                       
                    </div>
                </div>
               
                
                 <div class="row">
                    <div class="col-md-3">
                        <label class="control-label">Desripción</label>
                    </div>
                    <div class="col-md-8">
                        {{entity.descripcion}}
                    </div>
                </div>
 
    </div>

    </div>

<div class="col-lg-9">
    <div class="float-left col-lg-3">
        <a class="btn btn-info col-lg-11" href="{{ path('disposicion') }}">
            {{ 'views.recordactions.backtothelist'|trans({}, 'MWSimpleCrudGeneratorBundle') }}
        </a>
    </div>

</div>
{% endblock %}
