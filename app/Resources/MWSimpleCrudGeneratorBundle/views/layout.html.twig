<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>{% block title %}{{ 'views.layout.bannersadmin'|trans({}, 'MWSimpleCrudGeneratorBundle') }}{% endblock %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">

    <!-- Le styles -->

    <!-- Twitter Bootstrap -->
    <link href="{{ asset('bundles/mwsimplecrudgenerator/css/bootstrap.min.css') }}" rel="stylesheet">

    <!-- Material Design for Bootstrap -->
    <link href="{{ asset('bundles/mwsimplecrudgenerator/css/roboto.min.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/mwsimplecrudgenerator/css/material-fullpalette.min.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/mwsimplecrudgenerator/css/ripples.min.css') }}" rel="stylesheet">
     <!-- Datepicker Material -->

    <link href="{{ asset('bundles/mwsimplecrudgenerator/css/bootstrap-material-datetimepicker.css') }}" rel="stylesheet">


    <link href="{{ asset('bundles/mwsimplecrudgenerator/css/bootstrap-checkbox.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/mwsimplecrudgenerator/css/crud.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/mwsimplecrudgenerator/css/select2.min.css') }}" rel="stylesheet" />
    {% block stylesheets %}{% endblock %}
    <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

    <!-- Le fav and touch icons -->
    <link rel="shortcut icon" href="{{ asset('bundles/mwsimplecrudgenerator/images/favicon.ico') }}">
</head>

<body class="header">
		{% block menu %}
	           {% include "::menuBackend.html.twig" %}       
		{% endblock %}
<div class="container">

    
    {% for type, flashMessages in app.session.flashbag.all() %}
            {% for flashMessage in flashMessages %}
                <div class="alert alert-{{ type }}">
                    {{ flashMessage|trans({}, 'MWSimpleCrudGeneratorBundle') }}
                </div>
            {% endfor %}
    {% endfor %}

    {% block page %}{% endblock %}

</div>


{% include "::footer.html.twig" %}

<script src="{{ asset('bundles/mwsimplecrudgenerator/js/jquery.min.js') }}"></script>
<script src="{{ asset('bundles/mwsimplecrudgenerator/js/bootstrap.min.js') }}"></script>
<script src="{{ asset('bundles/mwsimplecrudgenerator/js/select2.min.js') }}"></script>
<script src="{{ asset('bundles/mwsimplecrudgenerator/js/angular.min.js') }}"></script>
<script src="{{ asset('bundles/mwsimplecrudgenerator/js/app.js') }}"></script>
<script src="{{ asset('bundles/mwsimplecrudgenerator/js/material.min.js') }}"></script>
<script src="{{ asset('bundles/mwsimplecrudgenerator/js/es.js') }}"></script>
<script src="{{ asset('bundles/mwsimplecrudgenerator/js/moment.min.js') }}"></script>
<script src="{{ asset('bundles/mwsimplecrudgenerator/js/bootstrap-material-datetimepicker.js') }}"></script>
<script src="{{ asset('bundles/mwsimplecrudgenerator/js/bootstrap-checkbox.js') }}"></script>
<script src="{{ asset('bundles/mwsimplecrudgenerator/js/ripples.min.js') }}"></script>
<script>
      $.material.init();
</script>

{% block javascript %}{% endblock %}
</body>
</html>